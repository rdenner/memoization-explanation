{
  "name": "redux-selectors",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/redux-selectors/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/redux-selectors"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "redux-selectors:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "redux-selectors:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "redux-selectors:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "redux-selectors:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "redux-selectors:build:development"
        },
        "production": {
          "buildTarget": "redux-selectors:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "coverage/libs/redux-selectors"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/redux-selectors"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/redux-selectors/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "redux-selectors:build"
      }
    }
  },
  "tags": []
}
